{
    "name": "oroessner/remote-php-kata",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ole Rößner",
            "email": "o.roessner@neusta.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kata\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kata\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    }
}
